{
  "expo": {
    "name": "azv-react",
    "slug": "azv-react",
    "version": "3.0.18",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "azvreact",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Приложение использует камеру для создания фотографий",
        "NSMicrophoneUsageDescription": "Приложение использует микрофон для записи видео",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.example.azvMotor"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "NSLocationWhenInUseUsageDescription",
        "ACCESS_FINE_LOCATION",
        "WRITE_EXTERNAL_STORAGE",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "kz.azv.motors",
      "versionCode": 20,
      "googleServicesFile": "./google-services.json",
      "minSdkVersion": 24,
      "targetSdkVersion": 35
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWeasth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Приложение использует камеру для создания фотографий",
          "microphonePermission": "Приложение использует микрофон для записи видео"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo.png",
          "color": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1741b81e-4164-4aaf-a288-b65573eea621"
      }
    },
    "owner": "meyirman_is_creator"
  }
}
